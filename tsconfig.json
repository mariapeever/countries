{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"target": "es2022",
		"allowJs": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"composite": true,
        "module": "es2022",
        "jsx": "react",
        "downlevelIteration": true
	},
	"exclude": ["src"]
}